{% extends 'AppBundle:Layouts:layout.html.twig' %}

{% block title %}
{{ parent() }} - lote
{% endblock %}

{% block content %}
<div class="col-md-12 col-sm-12 col-lg-12">
<div id="top" class="row">
    <div class="page-header">
        <h2>View lote <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span> </h2>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <p><strong>Id</strong></p>
        <p>
            {{ lote.id }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Container</strong></p>
        <p>
            {{ lote.container }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Numberpallets</strong></p>
        <p>
            {{ lote.numberPallets }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Templatenumber</strong></p>
        <p>
            {{ lote.templateNumber }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Packingdate</strong></p>
        <p>
            {{ lote.packingDate }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Label</strong></p>
        <p>
            {{ lote.label }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Comoditty</strong></p>
        <p>
            {{ lote.comoditty }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Variety</strong></p>
        <p>
            {{ lote.variety }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Pack</strong></p>
        <p>
            {{ lote.pack }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Plu</strong></p>
        <p>
            {{ lote.plu }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Quality</strong></p>
        <p>
            {{ lote.quality }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Score</strong></p>
        <p>
            {{ lote.score }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Numboxes</strong></p>
        <p>
            {{ lote.numBoxes }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Growercode</strong></p>
        <p>
            {{ lote.growerCode }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Growersname</strong></p>
        <p>
            {{ lote.growersName }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Exportador</strong></p>
        <p>
            {{ lote.exportador }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Consignne</strong></p>
        <p>
            {{ lote.consignne }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Vessel</strong></p>
        <p>
            {{ lote.vessel }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Pol</strong></p>
        <p>
            {{ lote.pol }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Etd</strong></p>
        <p>
            {{ lote.etd }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Pod</strong></p>
        <p>
            {{ lote.pod }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Eta</strong></p>
        <p>
            {{ lote.eta }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Fechacarga</strong></p>
        <p>
            {% if lote.fechaCarga %}{{ lote.fechaCarga|date('Y-m-d H:i:s') }}{% endif %}
        </p>

    </div></div>

<hr/>


<div class="form-group">
    <a class="btn btn-default" href="{{ path('lotes') }}">
        <span class="glyphicon glyphicon-list" aria-hidden="true"></span>
        Back to list
    </a>
    <a class="btn btn-primary" href="{{ path('lotes_edit', { 'id':  lote.id }) }}">
        <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
        Edit
    </a>
    <a class="btn btn-info" href="{{ path('lotes_new') }}">
        <span class="glyphicon glyphicon-plus" aria-hidden="true"></span>
        New
    </a>
    <form action="{{ path('lotes_delete', { 'id': lote.id }) }}" method="post" style="display: inline-block">
        <input type="hidden" name="_method" value="DELETE" />
        {{ form_widget(delete_form) }}
        <button class="btn btn-danger" type="submit" onclick="return confirm('Are you sure');">
            <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
            Delete
        </button>
    </form>
</div>

</div>
{% endblock %}

