{% extends 'AppBundle:Layouts:layout.html.twig' %}

{% block title %}
{{ parent() }} - general
{% endblock %}

{% block body %}

<div id="top" class="row">
    <div class="page-header">
        <h2>View general <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span> </h2>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <p><strong>Id</strong></p>
        <p>
            {{ general.id }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Inspector</strong></p>
        <p>
            {{ general.inspector }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Dateinspect</strong></p>
        <p>
            {% if general.dateinspect %}{{ general.dateinspect|date('Y-m-d H:i:s') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Inspectplan</strong></p>
        <p>
            {{ general.inspectplan }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Countryorigin</strong></p>
        <p>
            {{ general.countryorigin }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Commodityname</strong></p>
        <p>
            {{ general.commodityname }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Variety</strong></p>
        <p>
            {{ general.variety }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Productpackstyle</strong></p>
        <p>
            {{ general.productpackstyle }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Label</strong></p>
        <p>
            {{ general.label }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Casestype</strong></p>
        <p>
            {{ general.casestype }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Caseperpallet</strong></p>
        <p>
            {{ general.caseperpallet }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Grower</strong></p>
        <p>
            {{ general.grower }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Size</strong></p>
        <p>
            {{ general.size }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Packingdate</strong></p>
        <p>
            {% if general.packingdate %}{{ general.packingdate|date('Y-m-d H:i:s') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Casesnetweight</strong></p>
        <p>
            {{ general.casesnetweight }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Barcodeplu</strong></p>
